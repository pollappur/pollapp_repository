
<?xml version="1.0" encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        

<hibernate-mapping>

    <class entity-name="pl.model.Group" table="GROUP">

   
        <id name="id_group"
            type="long"
            column="ID_GROUP">
            <generator class="sequence"/>
        </id>

        <property name="name"
                  column="NAME"
                  type="string"/>

        <property name="year"
                  column="YEAR"
                  type="string"/>
        
        <property name="fiel_of_study"
                  column="TITLE"
                  type="string"/>
           
                  
    </class>
    
</hibernate-mapping>


